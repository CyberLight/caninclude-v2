{
  "private": true,
  "name": "caninclude",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node server",
    "build": "preact build",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest",
    "api:dev": "PORT=8081 node server"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ],
    "root": true
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^8.2.0",
    "eslint-config-preact": "^1.2.0",
    "eslint-plugin-compat": "^4.0.0",
    "jest": "^27.3.1",
    "jest-preset-preact": "^1.0.0",
    "preact-cli": "^0.1.0",
    "sirv-cli": "1.0.3"
  },
  "dependencies": {
    "caninclude-analyzer": "github:CyberLight/caninclude-analyzer#v1.0.1",
    "lodash": "^4.17.21",
    "polka": "^0.5.2",
    "preact": "^10.6.1",
    "preact-render-to-string": "^5.1.19",
    "preact-router": "^3.2.1",
    "sirv": "^1.0.18",
    "tailwindcss": "^2.2.19"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
